public class AccountsController { @AuraEnabled public static List<Account> getAccounts() { return [SELECT Id, name, industry, Type, NumberOfEmployees, TickerSymbol, Phone FROM Account ORDER BY createdDate DESC]; } @AuraEnabled public static List<Contact> getContacts(String AccountId){ return [SELECT Id, name FROM Contact Where AccountId =: AccountId]; } @AuraEnabled public static void createAccount(Account newAcc){ insert newAcc; } }